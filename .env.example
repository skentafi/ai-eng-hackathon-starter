# .env.example

# Optional: These variables override config.toml settings when present

# Logic variant to use for cost estimation (e.g., "default-logic", "linear-v2", "custom-tree")
LOGIC_VARIANT=default-logic

# Default currency for cost output (e.g., "USD", "EUR", "MAD")
DEFAULT_CURRENCY=USD

# S3 bucket name for optional file storage or logging
S3_BUCKET=default-bucket
